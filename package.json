{
  "name": "fmp4-mp4-remuxer",
  "version": "0.1.2",
  "description": "Browser/Node-friendly fMP4 fragments to MP4 remuxer (video track)",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "fmp4-mp4-remuxer": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run typecheck",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.2",
    "typescript": "^5.7.3"
  }
}
